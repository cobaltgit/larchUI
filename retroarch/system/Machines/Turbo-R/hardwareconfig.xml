<?xml version="1.0" ?>
<!DOCTYPE msxconfig SYSTEM 'msxconfig2.dtd'>
<msxconfig>

  <info>
    <manufacturer></manufacturer>
    <code>Turbo-R Japanese</code>
    <release_year></release_year>
    <description/>
    <type>Turbo-R</type>
  </info>

  <MapperReadBackBits>largest</MapperReadBackBits>

  <PanasonicRom id="Firmware ROM">
    <rom>
      <filename>fsa1gt.rom</filename>
    </rom>
  </PanasonicRom>

  <RenShaTurbo>
    <min_ints>47</min_ints>
    <max_ints>221</max_ints>
  </RenShaTurbo>

  <devices>

    <Kanji id="Kanji ROM">
      <rom>
        <filename>kanji.rom</filename>
      </rom>
      <io base="0xD8" num="2" type="O"/>
      <io base="0xD9" num="1" type="I"/>
      <io base="0xDA" num="2" type="O"/>
      <io base="0xDB" num="1" type="I"/>
    </Kanji>

    <S1990 id="S1990">
      <io base="0xE4" num="2"/>
    </S1990>

    <PPI id="ppi">
      <key_ghosting>true</key_ghosting>
      <sound>
        <volume>16000</volume>
      </sound>
      <io base="0xA8" num="4"/>
    </PPI>

    <VDP id="VDP">
      <version>V9958</version>
      <vram>128</vram>
      <io base="0x98" num="4" type="O"/>
      <io base="0x98" num="2" type="I"/>
    </VDP>

    <PSG id="PSG">
      <keyboardlayout>JIS</keyboardlayout>
      <sound>
        <volume>21000</volume>
      </sound>
      <io base="0xA0" num="2" type="O"/>
      <io base="0xA2" num="1" type="I"/>
    </PSG>

    <RTC id="Real time clock">
      <sramname>turbor.cmos</sramname>
      <io base="0xB4" num="2" type="O"/>
      <io base="0xB5" num="1" type="I"/>
    </RTC>

    <PrinterPort id="Printer Port">
      <io base="0x90" num="2"/>
    </PrinterPort>

    <E6Timer id="Turbo-R timer">
      <io base="0xE6" num="2"/>
    </E6Timer>

    <F4Device id="F4Device">
      <inverted>false</inverted>
      <io base="0xF4" num="1"/>
    </F4Device>

    <TurboRLeds id="TurboRLeds">
      <io base="0xA7" num="1" type="O"/>
    </TurboRLeds>

    <TurboRPause id="TurboRPause">
      <io base="0xA7" num="1" type="I"/>
    </TurboRPause>

    <TurboRPCM id="PCM">
      <io base="0xA4" num="2"/>
      <sound>
        <volume>21000</volume>
      </sound>
    </TurboRPCM>

    <MSX-MIDI id="MSX-Midi">
      <io base="0xE8" num="8"/>
    </MSX-MIDI>

    <MSX-AUDIO id="Generic MSX-Audio">
      <io base="0xC0" num="2"/>
      <sound>
        <volume>12000</volume>
        <mode>right</mode>
      </sound>
      <sampleram>256</sampleram>
    </MSX-AUDIO>

    <MusicModuleMIDI id="Generic MSX-Audio-MIDI">
      <io base="0x00" num="2" type="O"/>
      <io base="0x04" num="2" type="I"/>
    </MusicModuleMIDI>

    <MoonSound id="Sunrise MoonSound">
      <io base="0x7e" num="2"/>
      <io base="0xc4" num="4"/>
      <rom>
        <filename>moonsound.rom</filename>
      </rom>
      <sound>
        <volume>12000</volume>
      </sound>
      <sampleram>640</sampleram>
    </MoonSound>

    <V9990 id="Sunrise GFX9000">
      <io base="0x60" num="0x10"/>
    </V9990>

    <primary slot="0">

      <secondary slot="0">
        <ROM id="MSX BIOS with BASIC ROM">
          <mem base="0x0000" size="0x8000"/>
          <mappertype>DRAM</mappertype>
          <rom>
            <firstblock>40</firstblock>
            <lastblock>43</lastblock>
          </rom>
        </ROM>
      </secondary>

      <secondary slot="2">
        <MSX-MUSIC id="MSX Music">
          <mem base="0x4000" size="0x4000"/>
          <io base="0x7C" num="2" type="O"/>
          <rom>
            <firstblock>62</firstblock>
            <lastblock>63</lastblock>
          </rom>
          <sound>
            <volume>13000</volume>
            <mode>left</mode>
          </sound>
        </MSX-MUSIC>
      </secondary>

      <secondary slot="3">
        <ROM id="MSX Opening ROM">
          <mem base="0x4000" size="0x4000"/>
          <rom>
            <firstblock>36</firstblock>
            <lastblock>37</lastblock>
          </rom>
        </ROM>
      </secondary>

    </primary>

    <primary external="true" slot="1"/>

    <primary external="true" slot="2"/>

    <primary slot="2">

      <secondary slot="1">
        <MegaRam id="MegaRam">
          <io base="0x8e" num="1"/>
          <mem base="0x0000" size="0x10000"/>
          <size>2048</size>
        </MegaRam>
      </secondary>

      <secondary slot="2">
        <PAC id="PanaSoft SW-M001 PAC">
          <mem base="0x4000" size="0x4000"/>
          <sramname>turbor.pac</sramname>
        </PAC>
      </secondary>

      <secondary slot="3">
        <ROM id="MSX Turbo-Basic 2">
          <rom>
            <filename>xbasic2.rom</filename>
          </rom>
          <mem base="0x4000" size="0x8000"/>
        </ROM>        
      </secondary>

    </primary>

    <primary slot="3">

      <secondary slot="0">
        <PanasonicRAM id="Main RAM">
          <mem base="0x0000" size="0x10000"/>
          <size>2048</size>
        </PanasonicRAM>
      </secondary>

      <secondary slot="1">
        <ROM id="MSX Sub ROM">
          <mem base="0x0000" size="0x4000"/>
          <mappertype>DRAM</mappertype>
          <rom>
            <firstblock>56</firstblock>
            <lastblock>57</lastblock>
          </rom>
        </ROM>
        <ROM id="MSX Kanji Driver with BASIC">
          <mem base="0x4000" size="0x8000"/>
          <mappertype>DRAM</mappertype>
          <rom>
            <firstblock>58</firstblock>
            <lastblock>61</lastblock>
          </rom>
        </ROM>
      </secondary>

      <secondary slot="2">
        <TC8566AF id="Memory Mapped FDC">
          <mem base="0x4000" size="0x8000"/>
          <rom>
            <firstblock>48</firstblock>
            <lastblock>55</lastblock>
          </rom>
          <drives>2</drives>
        </TC8566AF>
      </secondary>

      <secondary slot="3">
        <ROM id="Firmware">
          <mem base="0x0000" size="0x10000"/>
          <mappertype>PANASONIC</mappertype>
          <rom>
            <firstblock>0</firstblock>
            <lastblock>511</lastblock>
          </rom>
          <sramsize>32</sramsize>
          <sram-mirrored>false</sram-mirrored>
          <sramname>turbor.sram</sramname>
        </ROM>
      </secondary>

    </primary>

  </devices>

</msxconfig>
